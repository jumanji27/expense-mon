body
  margin: 0
  color: #5a5a5a
  div
    display: flex
    box-sizing: border-box
h1, h2, h3, h4, h5, h6
  margin:0
img
  display: block
  border: none
p
  margin: 0
input, textarea, button
  display: block
  border: none
  outline: none
  color: #5a5a5a
  margin: 0
  padding: 0
button
  cursor: pointer
  background: #fff
label
  display: block
ul
  list-style: none
  margin: 0
  padding: 0
code
  display: block